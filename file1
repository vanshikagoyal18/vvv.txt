//array insertion
#include <iostream.h> 
using namespace std; 
int main(){ 
 int LA[3] = {}, i; 
 cout << "Array Before Insertion:" << endl; 
 for(i = 0; i < 3; i++) 
 cout << "LA[" << i <<"] = " << LA[i] << endl; 
 
 //prints garbage values 
 cout << "Inserting elements.." <<endl; 
 cout << "Array After Insertion:" << endl; // prints array values 
 for(i = 0; i < 3; i++) { 
 LA[i] = i + 1; 
 cout << "LA[" << i <<"] = " << LA[i] << endl; 
 } 
 return 0; 
} 
//array elements:{1,2,3}
